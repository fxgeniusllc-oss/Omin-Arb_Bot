# OmniArbBot Configuration Example
# Copy this file to .env and fill in your values

# RPC Endpoints (comma-separated for multiple chains)
RPC_ENDPOINTS=https://mainnet.infura.io/v3/YOUR_API_KEY,https://bsc-dataseed.binance.org/

# Wallet Configuration
PRIVATE_KEY=your_private_key_here

# Trading Parameters
MIN_PROFIT_THRESHOLD=0.01
MAX_TRADE_AMOUNT=1.0
GAS_LIMIT=300000

# Bot Settings
SCAN_INTERVAL=5
ENABLE_AUTO_TRADING=false
